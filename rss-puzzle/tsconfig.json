{
  "compilerOptions": {
    "target": "es2016",
    "moduleResolution": "Node16",
    "module": "Node16",
    "resolvePackageJsonExports": true,
    "resolvePackageJsonImports": true,
    "customConditions": [],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}
